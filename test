Tabs.Farm:Section({ Title = "Fishing" })

-- Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local UserInputService = game:GetService("UserInputService")
local VirtualInputManager = game:GetService("VirtualInputManager")
local RunService = game:GetService("RunService")

local function GetFishingTool()
    local rodName
    pcall(function()
        rodName = ReplicatedStorage.playerstats[LocalPlayer.Name].Stats.rod.Value
    end)

    local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local backpack = LocalPlayer:FindFirstChildOfClass("Backpack")

    -- Check in Character (equipped)
    for _, v in ipairs(char:GetChildren()) do
        if rodName and v.Name == rodName then
            return v
        elseif v.Name:lower():find("rod") or v.Name:lower():find("fish") then
            return v
        end
    end

    -- Check in Backpack (not equipped)
    if backpack then
        for _, v in ipairs(backpack:GetChildren()) do
            if rodName and v.Name == rodName then
                return v
            elseif v.Name:lower():find("rod") or v.Name:lower():find("fish") then
                return v
            end
        end
    end

    -- Wait briefly for replication
    local startTime = tick()
    while tick() - startTime < 2 do
        task.wait(0.1)
        if backpack then
            for _, v in ipairs(backpack:GetChildren()) do
                if rodName and v.Name == rodName then
                    return v
                elseif v.Name:lower():find("rod") or v.Name:lower():find("fish") then
                    return v
                end
            end
        end
    end

    return nil
end

