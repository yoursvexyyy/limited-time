Tabs.Shop:Dropdown({
    Title = "Select Rod",
    Values = premiumRodList,
    SearchBarEnabled = true,
    Value = chosenPremiumRod,
    Callback = function(option)
        chosenPremiumRod = option
    end
})

Tabs.Shop:Button({
    Title = "Purchase Selected Rod",
    Callback = function()
        if chosenPremiumRod ~= "" then
            game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("purchase"):FireServer(chosenPremiumRod, "Rod", 1)
            
            WindUI:Notify({
                Title = "Rod Purchase",
                Content = "Purchased: " .. chosenPremiumRod,
                Duration = 5,
                Icon = "shopping-bag"
            })
        else
            WindUI:Notify({
                Title = "Purchase Failed",
                Content = "Please select a rod first!",
                Duration = 5,
                Icon = "alert-circle"
            })
        end
    end
})

