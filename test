local chosenLocation = nil

local locationNames = {}
for name, _ in pairs(islandLocations) do
    table.insert(locationNames, name)
end

Tabs.Teleport:Dropdown({
    Title = "Select Location",
    Values = locationNames,
    SearchBarEnabled = true,
    Value = locationNames[1],
    Callback = function(option)
        chosenLocation = option
    end
})

Tabs.Teleport:Button({
    Title = "Teleport",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        if chosenLocation and islandLocations[chosenLocation] then
            character.HumanoidRootPart.CFrame = islandLocations[chosenLocation]
            WindUI:Notify({
                Title = "Teleport",
                Content = "Teleported to " .. chosenLocation,
                Duration = 5,
                Icon = "map-pin",
            })
        else
            WindUI:Notify({
                Title = "Teleport",
                Content = "Please select a valid location!",
                Duration = 5,
                Icon = "map",
            })
        end
    end
})

Tabs.Teleport:Section({ Title = "NPCs" })

