    Title = "Teleport",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        if chosenNPC and npcLocations[chosenNPC] then
            character.HumanoidRootPart.CFrame = npcLocations[chosenNPC]
            WindUI:Notify({
                Title = "Teleport",
                Content = "Teleported to " .. chosenNPC,
                Duration = 5,
                Icon = "map-pin",
            })
        else
            WindUI:Notify({
                Title = "Teleport",
                Content = "Please select a valid NPC!",
                Duration = 5,
                Icon = "map",
            })
        end
    end
})

