            :WaitForChild("RE/Bobber/Equip")
            :FireServer(unpack(args))
    end
})

Tabs.Farm:Section({ Title = "Sell" })

local AutoSellEnabled = false
local SellWaitTime = 5

Tabs.Farm:Slider({
    Title = "Sell Delay",
    Step = 1,
    Value = {
        Min = 1,
        Max = 100,
        Default = 5,
    },
    Callback = function(value)
        SellWaitTime = value
    end
})

Tabs.Farm:Toggle({
    Title = "Enable Auto Sell",
    Value = false,
    Callback = function(state)
        AutoSellEnabled = state
    end
})

local Events = ReplicatedStorage:WaitForChild("events")
local npcFolder = workspace:WaitForChild("world"):WaitForChild("npcs")

local function FindMerchant()
    local npc = nil
    while not npc do
        for _, v in ipairs(npcFolder:GetChildren()) do
            if v:IsA("Model") and string.match(v.Name, "Merchant$") then
                npc = v
                break
            end
        end
        if not npc then
            npcFolder.ChildAdded:Wait()
        end
    end
    return npc
end

task.spawn(function()
    while task.wait() do
        if AutoSellEnabled then
            local npc = FindMerchant()
            local idle = npc:FindFirstChild("description") and npc.description:FindFirstChild("idle")

            if npc and idle then
                local args = {
                    {
                        voice = 12,
                        npc = npc,
                        idle = idle
                    }
                }
                Events:WaitForChild("SellAll"):InvokeServer(unpack(args))
            end

            task.wait(SellWaitTime)
        end
    end
end)

Tabs.Shop:Section({ Title = "Crate" })

local chosenCrate = "Bait Crate"
local crateCount = 5
local AutoPurchase = false

Tabs.Shop:Dropdown({ 
    Title = "Select Crate", 
    Values = { "Bait Crate", "Quality Bait Crate" }, 
    Value = chosenCrate, 
    Callback = function(option) 
        chosenCrate = option
    end
})

Tabs.Shop:Toggle({
    Title = "Auto Buy Crate",
    Value = false,
    Callback = function(state)
        AutoPurchase = state
    end
})

local purchaseEvent = ReplicatedStorage:WaitForChild("events"):WaitForChild("purchase")

spawn(function()
    while true do
        wait(0.01)
        if AutoPurchase then
            local args = {
                chosenCrate,
                "Fish",
                [4] = crateCount
            }
            purchaseEvent:FireServer(unpack(args))
        end
    end
end)

