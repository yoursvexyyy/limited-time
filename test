-- Perfect Auto Cast Variables
local AutoCastEnabled = false
local CastPowerThreshold = 0.95
local CastLoop = nil
local IsHoldingCast = false

-- Continuous holding variables
local LastHoldTime = 0
local HoldInterval = 1
local InFishingGame = false

-- Function to check if UI is active
local function IsFishingGameActive()
    local playerGui = game.Players.LocalPlayer:FindFirstChild("PlayerGui")
    if not playerGui then return false end
    
    if playerGui:FindFirstChild("shakeui") then
        return true
    end
    
    if playerGui:FindFirstChild("reel") then
        return true
    end
    
    return false
end

-- Function to check if rod is equipped
local function HasRodEquipped()
    local character = LocalPlayer.Character
    if not character then return false end
    
    for _, tool in ipairs(character:GetChildren()) do
        if tool:IsA("Tool") and (
            string.find(tool.Name:lower(), "rod") or 
            string.find(tool.Name:lower(), "rend") or
            string.find(tool.Name:lower(), "ravager") or
            string.find(tool.Name:lower(), "yarncaster") or
            string.find(tool.Name:lower(), "boom") or
            string.find(tool.Name:lower(), "oath") or
            string.find(tool.Name:lower(), "staff") or
            string.find(tool.Name:lower(), "duskwire") or
            string.find(tool.Name:lower(), "spiritbinder") or
            string.find(tool.Name:lower(), "pitchfork") or
            string.find(tool.Name:lower(), "mossripper") or
            string.find(tool.Name:lower(), "onirifalx") or
            string.find(tool.Name:lower(), "dreambreaker") or
            string.find(tool.Name:lower(), "blazer") or
            string.find(tool.Name:lower(), "fang") or
            string.find(tool.Name:lower(), "fallen") or
            string.find(tool.Name:lower(), "wingripper") or
            string.find(tool.Name:lower(), "serenade") or
            string.find(tool.Name:lower(), "cinderstring") or
            string.find(tool.Name:lower(), "eardrum") or
            string.find(tool.Name:lower(), "chrysalis") or
            string.find(tool.Name:lower(), "axe") or
            string.find(tool.Name:lower(), "blade") or
            string.find(tool.Name:lower(), "guitar") or
            string.find(tool.Name:lower(), "sword") or
            string.find(tool.Name:lower(), "spire") or
            string.find(tool.Name:lower(), "paintbrush") or
            string.find(tool.Name:lower(), "nilcaster") or
            string.find(tool.Name:lower(), "fish") or
            string.find(tool.Name:lower(), "smasher") or
            string.find(tool.Name:lower(), "caster") or
            string.find(tool.Name:lower(), "sovereign") or
            string.find(tool.Name:lower(), "elemental")
        ) then
            return true
        end
    end
    return false
end

-- Function to find the power bar
local function GetPowerBar()
    local character = LocalPlayer.Character
    if not character then return nil end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return nil end
    
    local power = humanoidRootPart:FindFirstChild("power")
    if not power then return nil end
    
    local powerbar = power:FindFirstChild("powerbar")
    if not powerbar then return nil end
    
    local bar = powerbar:FindFirstChild("bar")
    return bar
end

-- Function to get current cast power
local function GetCastPower()
    local bar = GetPowerBar()
    if bar and bar:IsA("Frame") then
        return bar.Size.Y.Scale
    end
    return 0
end

-- Function to simulate touch hold
local function StartCastHold()
    if IsHoldingCast then return end
    
    local screenSize = workspace.CurrentCamera.ViewportSize
    local holdX = screenSize.X - 100
    local holdY = screenSize.Y - 100
    
    pcall(function()
        VirtualInputManager:SendMouseButtonEvent(
            holdX,
            holdY,
            0,
            true,
            game,
            1
        )
        IsHoldingCast = true
    end)
end

-- Function to simulate touch release
local function ReleaseCast()
    if not IsHoldingCast then return end
    
    local screenSize = workspace.CurrentCamera.ViewportSize
    local screenCenterX = screenSize.X - 100
    local screenCenterY = screenSize.Y - 100
    
    pcall(function()
        VirtualInputManager:SendMouseButtonEvent(
            screenCenterX,
            screenCenterY,
            0,
            false,
            game,
            1
        )
        IsHoldingCast = false
        print("VEX IS BEST!")
    end)
end

-- Main Auto Cast Loop
local function StartAutoCast()
    local HasCast = false
    local CastCooldown = false
    
    CastLoop = RunService.Heartbeat:Connect(function()
        if not AutoCastEnabled then return end
        
        if not HasRodEquipped() then
            if IsHoldingCast then
                ReleaseCast()
            end
            HasCast = false
            CastCooldown = false
            return
        end
        
        InFishingGame = IsFishingGameActive()
        
        if InFishingGame then
            if IsHoldingCast then
                ReleaseCast()
            end
            return
        end
        
        local CurrentPower = GetCastPower()
        local CurrentTime = tick()
        
        if not IsHoldingCast and CurrentPower == 0 and not HasCast and not CastCooldown then
            if CurrentTime - LastHoldTime >= HoldInterval then
                StartCastHold()
                LastHoldTime = CurrentTime
            end
        end
        
        if IsHoldingCast and CurrentPower > 0 then
            if CurrentPower >= CastPowerThreshold and not HasCast then
                ReleaseCast()
                HasCast = true
                CastCooldown = true
                wait(3)
                HasCast = false
                CastCooldown = false
            end
        end
        
        if IsHoldingCast and CurrentPower == 0 and CurrentTime - LastHoldTime >= HoldInterval then
            ReleaseCast()
        end
    end)
end

-- Stop Auto Cast
local function StopAutoCast()
    if CastLoop then
        CastLoop:Disconnect()
        CastLoop = nil
    end
    if IsHoldingCast then
        ReleaseCast()
    end
    IsHoldingCast = false
    InFishingGame = false
end

-- Perfect Auto Cast Toggle
Tabs.Farm:Toggle({
    Title = "Auto Perfect Cast",
    Value = false,
    Callback = function(state)
        AutoCastEnabled = state
        
        if AutoCastEnabled then
            if HasRodEquipped() then
                StartAutoCast()
            else
                AutoCastEnabled = false
                WindUI:Notify({
                    Title = "No Rod Equipped",
                    Content = "Please equip a fishing rod first!",
                    Duration = 3,
                    Icon = "alert-triangle",
                })
            end
        else
            StopAutoCast()
        end
    end
})

-- Auto Shake
local AutoShakeEnabled = false

Tabs.Farm:Toggle({
    Title = "Auto Shake",
    Value = false,
    Callback = function(state)
        AutoShakeEnabled = state
    end
})

-- Shake Function
local function PerformShake()
    local gui = LocalPlayer:WaitForChild("PlayerGui")
    local shakeui = gui:FindFirstChild("shakeui")
    if shakeui then
        local safezone = shakeui:FindFirstChild("safezone")
        if safezone then
            local button = safezone:FindFirstChild("button")
            if button and button:FindFirstChild("shake") then
                button.shake:FireServer()
            end
        end
    end
end

-- Shake Loop
task.spawn(function()
    while true do
        task.wait(0.1)
        if AutoShakeEnabled then
            pcall(PerformShake)
        end
    end
end)

-- Auto Reel - Modified Version with 1 Second Delay
local AutoReelEnabled = false
local lastPlayerBarPosition = nil
local movementDetected = false
local movementTime = 0
local reelEvent = game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished")
local reelArgs = {100, true}

-- Function to get positions
local function GetReelUI()
    local gui = game.Players.LocalPlayer:FindFirstChild("PlayerGui")
    if not gui then return nil end
    
    local reel = gui:FindFirstChild("reel")
    if not reel then return nil end
    
    local bar = reel:FindFirstChild("bar")
    if not bar then return nil end
    
    local playerBar = bar:FindFirstChild("playerbar")
    local fishLine = bar:FindFirstChild("fish")
    
    if not playerBar or not fishLine then return nil end
    
    return {
        playerBar = playerBar,
        fishLine = fishLine
    }
end

Tabs.Farm:Toggle({
    Title = "Auto Reel + Perfect Catch",
    Value = false,
    Callback = function(state)
        AutoReelEnabled = state
        lastPlayerBarPosition = nil
        movementDetected = false
        movementTime = 0
    end
})

-- Modified Reel Loop with 1 Second Delay
task.spawn(function()
    while task.wait(0.05) do
        if AutoReelEnabled then
            local uiElements = GetReelUI()
            if uiElements then
                local playerBar = uiElements.playerBar
                local currentPosition = playerBar.Position
                
                if not lastPlayerBarPosition then
                    -- First time detecting the minigame, store initial position
                    lastPlayerBarPosition = currentPosition
                    movementDetected = false
                    movementTime = 0
                    print("Auto Reel: Minigame detected, monitoring for movement...")
                else
                    -- Check if player bar has moved from its initial position
                    if currentPosition.X.Offset ~= lastPlayerBarPosition.X.Offset or
                       currentPosition.X.Scale ~= lastPlayerBarPosition.X.Scale then
                        
                        if not movementDetected then
                            -- First time detecting movement
                            movementDetected = true
                            movementTime = tick()
                            print("Auto Reel: Movement detected! Waiting 1 second...")
                        else
                            -- Movement already detected, check if 1 second has passed
                            local timeSinceMovement = tick() - movementTime
                            if timeSinceMovement >= 1 then
                                -- 1 second has passed, fire the remote
                                reelEvent:FireServer(unpack(reelArgs))
                                print("Auto Reel: 1 second passed! Instant reel fired.")
                                
                                -- Reset for next minigame
                                lastPlayerBarPosition = nil
                                movementDetected = false
                                movementTime = 0
                                task.wait(0.5) -- Small cooldown
                            end
                        end
                    else
                        -- Player bar hasn't moved yet or returned to original position
                        if movementDetected then
                            -- Reset if bar moved back to original position
                            movementDetected = false
                            movementTime = 0
                        end
                    end
                end
            else
                -- No minigame UI detected, reset tracking
                lastPlayerBarPosition = nil
                movementDetected = false
                movementTime = 0
            end
        else
            -- Auto reel disabled, reset tracking
            lastPlayerBarPosition = nil
            movementDetected = false
            movementTime = 0
        end
    end
end)

-- // Instant Reel Button (keep this as backup)
Tabs.Farm:Button({
    Title = "Instant Reel + Perfect Catch",
    Callback = function()
        reelEvent:FireServer(unpack(reelArgs))
    end
})

