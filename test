local chosenRod = nil

local rodNames = {}
for name, _ in pairs(rodLocations) do
    table.insert(rodNames, name)
end

Tabs.Teleport:Dropdown({
    Title = "Select Rod",
    Values = rodNames,
    SearchBarEnabled = true,
    Value = rodNames[1],
    Callback = function(option)
        chosenRod = option
    end
})

Tabs.Teleport:Button({
    Title = "Teleport",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        if chosenRod and rodLocations[chosenRod] then
            character.HumanoidRootPart.CFrame = rodLocations[chosenRod]
            WindUI:Notify({
                Title = "Teleport",
                Content = "Teleported to " .. chosenRod,
                Duration = 5,
                Icon = "map-pin",
            })
        else
            WindUI:Notify({
                Title = "Teleport",
                Content = "Please select a valid rod!",
                Duration = 5,
                Icon = "map",
            })
        end
    end
})

-- Water Walking Platform
local WaterWalkingEnabled = false
local WaterPlatformPart = nil
local PlatformFollow = false

Tabs.Misc:Toggle({
    Title = "Walk on Water/Air",
    Description = "Creates invisible platform to walk on water or through air",
    Value = false,
    Callback = function(state)
        WaterWalkingEnabled = state
        
        if WaterWalkingEnabled then
            -- Create platform only when toggle is enabled
            local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
            local HumanoidRoot = Character:WaitForChild("HumanoidRootPart")
            
            WaterPlatformPart = Instance.new("Part")
            WaterPlatformPart.Name = "WaterWalkPlatform"
            WaterPlatformPart.Size = Vector3.new(10, 1, 10)
            WaterPlatformPart.Anchored = true
            WaterPlatformPart.CanCollide = true
            WaterPlatformPart.Material = Enum.Material.Neon
            WaterPlatformPart.Color = Color3.fromRGB(0, 162, 255)
            WaterPlatformPart.Transparency = 1
            WaterPlatformPart.Parent = workspace

            WaterPlatformPart.Position = Vector3.new(
                HumanoidRoot.Position.X,
                HumanoidRoot.Position.Y - 3,
                HumanoidRoot.Position.Z
            )

            PlatformFollow = true
            
            -- Platform follow loop
            local followLoop = RunService.RenderStepped:Connect(function()
                if PlatformFollow and HumanoidRoot and WaterPlatformPart then
                    local currentPos = WaterPlatformPart.Position
                    WaterPlatformPart.Position = Vector3.new(
                        HumanoidRoot.Position.X,
                        currentPos.Y,
                        HumanoidRoot.Position.Z
                    )
                end
            end)
            
            -- Store connection to disconnect later
            getgenv().WaterWalkConnection = followLoop
            
        else
            PlatformFollow = false
            if getgenv().WaterWalkConnection then
                getgenv().WaterWalkConnection:Disconnect()
                getgenv().WaterWalkConnection = nil
            end
            if WaterPlatformPart then
                WaterPlatformPart:Destroy()
                WaterPlatformPart = nil
            end
        end
    end
})

-- Infinite Oxygen
local InfiniteO2Enabled = false

Tabs.Misc:Toggle({
    Title = "Infinite Oxygen",
    Value = false,
    Callback = function(state)
        InfiniteO2Enabled = state
        
        if InfiniteO2Enabled then
            local character = game.Players.LocalPlayer.Character
            if character and character:FindFirstChild("Resources") then
                local oxygen = character.Resources:FindFirstChild("oxygen")
                if oxygen then
                    oxygen.Enabled = false
                end
            end
            WindUI:Notify({
                Title = "VEX",
                Content = "Infinite Oxygen Enabled!",
                Duration = 3,
                Icon = "wind"
            })
        else
            local character = game.Players.LocalPlayer.Character
            if character and character:FindFirstChild("Resources") then
                local oxygen = character.Resources:FindFirstChild("oxygen")
                if oxygen then
                    oxygen.Enabled = true
                end
            end
            WindUI:Notify({
                Title = "VEX",
                Content = "Infinite Oxygen Disabled!",
                Duration = 3,
                Icon = "wind"
            })
        end
    end
})

-- Handle character respawns for oxygen
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    task.wait(1)
    if InfiniteO2Enabled then
        if character:FindFirstChild("Resources") then
            local oxygen = character.Resources:FindFirstChild("oxygen")
            if oxygen then
                oxygen.Enabled = false
            end
        end
    end
end)

-- No Clip Toggle
local NoClipEnabled = false
local NoClipConnection = nil

Tabs.Misc:Toggle({
    Title = "No Clip",
    Description = "Walk through walls and objects",
    Value = false,
    Callback = function(state)
        NoClipEnabled = state
        
        if NoClipEnabled then
            -- Enable No Clip
            local character = game.Players.LocalPlayer.Character
            if character then
                -- Set all parts to CanCollide false
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
            
            -- Start continuous No Clip loop
            NoClipConnection = RunService.Stepped:Connect(function()
                if NoClipEnabled then
                    local character = game.Players.LocalPlayer.Character
                    if character then
                        for _, part in pairs(character:GetDescendants()) do
                            if part:IsA("BasePart") then
                                part.CanCollide = false
                            end
                        end
                    end
                else
                    -- Disconnect if disabled
                    if NoClipConnection then
                        NoClipConnection:Disconnect()
                        NoClipConnection = nil
                    end
                end
            end)
            
            WindUI:Notify({
                Title = "No Clip",
                Content = "No Clip Enabled!",
                Duration = 3,
                Icon = "user-x"
            })
        else
            -- Disable No Clip
            if NoClipConnection then
                NoClipConnection:Disconnect()
                NoClipConnection = nil
            end
            
            -- Restore collision
            local character = game.Players.LocalPlayer.Character
            if character then
                for _, part in pairs(character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = true
                    end
                end
            end
            
            WindUI:Notify({
                Title = "No Clip",
                Content = "No Clip Disabled!",
                Duration = 3,
                Icon = "user-check"
            })
        end
    end
})

-- Handle character respawns for No Clip
game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    task.wait(1) -- Wait for character to fully load
    if NoClipEnabled then
        -- Apply No Clip to new character
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

local AntiAFKEnabled = false

Tabs.Misc:Toggle({
    Title = "Enable Anti AFK",
    Description = "Prevents getting kicked for being idle",
    Value = false,
    Callback = function(state)
        AntiAFKEnabled = state
        
        if AntiAFKEnabled then
            WindUI:Notify({
                Title = "Anti AFK",
                Content = "Anti AFK Enabled! You won't get kicked for being idle.",
                Duration = 3,
                Icon = "user-check"
            })
        else
            WindUI:Notify({
                Title = "Anti AFK",
                Content = "Anti AFK Disabled!",
                Duration = 3,
                Icon = "user-x"
            })
        end
    end
})

-- Anti AFK Loop
local VirtualInputManager = game:GetService("VirtualInputManager")
local lastAFKTime = tick()

task.spawn(function()
    while true do
        task.wait(30) -- Check every 30 seconds
        if AntiAFKEnabled then
            local currentTime = tick()
            
            -- Simulate a small mouse movement every 10 minutes to prevent AFK
            if currentTime - lastAFKTime >= 600 then -- 10 minutes
                pcall(function()
                    local screenSize = workspace.CurrentCamera.ViewportSize
                    VirtualInputManager:SendMouseMoveEvent(
                        math.random(50, screenSize.X - 50),
                        math.random(50, screenSize.Y - 50),
                        game
                    )
                    lastAFKTime = currentTime
                    print("Anti-AFK: Simulated mouse movement")
                end)
            end
            
            -- Alternative method: Press a key
            pcall(function()
                VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Space, false, game)
                task.wait(0.1)
                VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Space, false, game)
            end)
        end
    end
end)

-- Floating button
local TweenService = game:GetService("TweenService")

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui

local ImageButton = Instance.new("ImageButton")
ImageButton.Parent = ScreenGui
ImageButton.Size = UDim2.new(0, 65, 0, 65)
ImageButton.Position = UDim2.new(0, 100, 1, -300)
ImageButton.BackgroundTransparency = 1
ImageButton.Image = "rbxassetid://82233980503615"

local UICorner = Instance.new("UICorner")
UICorner.CornerRadius = UDim.new(0, 15)
UICorner.Parent = ImageButton

local WindowOpen = false
ImageButton.MouseButton1Click:Connect(function()
    local shrink = TweenService:Create(ImageButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 50, 0, 50)
    })
    local grow = TweenService:Create(ImageButton, TweenInfo.new(0.1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 60, 0, 60)
    })

    shrink:Play()
    shrink.Completed:Wait()
    grow:Play()

    if WindowOpen then
        Window:Close()
    else
        Window:Open()
    end
    WindowOpen = not WindowOpen
end)

local UserInputService = game:GetService("UserInputService")
local isDragging = false
local dragInput, dragStart, startPosition

local function updatePosition(input)
    local delta = input.Position - dragStart
    ImageButton.Position = UDim2.new(
        startPosition.X.Scale, startPosition.X.Offset + delta.X,
        startPosition.Y.Scale, startPosition.Y.Offset + delta.Y
    )
end

ImageButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 
    or input.UserInputType == Enum.UserInputType.Touch then
        isDragging = true
        dragStart = input.Position
        startPosition = ImageButton.Position

        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                isDragging = false
            end
        end)
    end
end)

ImageButton.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement 
    or input.UserInputType == Enum.UserInputType.Touch then
        dragInput = input
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input == dragInput and isDragging then
        updatePosition(input)
    end
end)
